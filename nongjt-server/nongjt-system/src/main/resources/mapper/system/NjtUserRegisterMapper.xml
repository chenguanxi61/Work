<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yaorange.system.mapper.NjtUserRegisterMapper">

    <resultMap type="NjtUserRegister" id="NjtUserRegisterResult">
        <result property="id"    column="id"    />
        <result property="companyName"    column="company_name"    />
        <result property="mobilePhone"    column="mobile_phone"    />
        <result property="password"    column="password"    />
        <result property="registerTime"    column="register_time"    />
        <result property="memberFlag"    column="member_flag"    />
        <result property="perfectFlag"    column="perfect_flag"    />
        <result property="perfectTime"    column="perfect_time"    />
        <result property="probationTime"    column="probation_time"    />
        <result property="memberTime"    column="member_time"    />
        <result property="memberExpiredTime"    column="member_expired_time"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="userStatus"    column="user_status"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
    </resultMap>

    <sql id="selectNjtUserRegisterVo">
        select id, company_name, mobile_phone, password, register_time, member_flag, perfect_flag, perfect_time, probation_time, member_time, member_expired_time, del_flag, user_status, create_by, create_time, update_by, update_time from njt_user_register
    </sql>

    <select id="selectNjtUserRegisterList" parameterType="NjtUserRegister" resultMap="NjtUserRegisterResult">
        <include refid="selectNjtUserRegisterVo"/>
        <where>
            <if test="companyName != null  and companyName != ''"> and company_name like concat('%', #{companyName}, '%')</if>
            <if test="mobilePhone != null  and mobilePhone != ''"> and mobile_phone = #{mobilePhone}</if>
            <if test="password != null  and password != ''"> and password = #{password}</if>
            <if test="registerTime != null "> and register_time = #{registerTime}</if>
            <if test="memberFlag != null "> and member_flag = #{memberFlag}</if>
            <if test="perfectFlag != null "> and perfect_flag = #{perfectFlag}</if>
            <if test="perfectTime != null "> and perfect_time = #{perfectTime}</if>
            <if test="probationTime != null "> and probation_time = #{probationTime}</if>
            <if test="memberTime != null "> and member_time = #{memberTime}</if>
            <if test="memberExpiredTime != null "> and member_expired_time = #{memberExpiredTime}</if>
            <if test="userStatus != null "> and user_status = #{userStatus}</if>
        </where>
    </select>

    <select id="selectNjtUserRegisterById" parameterType="Long" resultMap="NjtUserRegisterResult">
        <include refid="selectNjtUserRegisterVo"/>
        where id = #{id}
    </select>

    <insert id="insertNjtUserRegister" parameterType="NjtUserRegister" useGeneratedKeys="true" keyProperty="id">
        insert into njt_user_register
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="companyName != null">company_name,</if>
            <if test="mobilePhone != null">mobile_phone,</if>
            <if test="password != null">password,</if>
            <if test="registerTime != null">register_time,</if>
            <if test="memberFlag != null">member_flag,</if>
            <if test="perfectFlag != null">perfect_flag,</if>
            <if test="perfectTime != null">perfect_time,</if>
            <if test="probationTime != null">probation_time,</if>
            <if test="memberTime != null">member_time,</if>
            <if test="memberExpiredTime != null">member_expired_time,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="userStatus != null">user_status,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="companyName != null">#{companyName},</if>
            <if test="mobilePhone != null">#{mobilePhone},</if>
            <if test="password != null">#{password},</if>
            <if test="registerTime != null">#{registerTime},</if>
            <if test="memberFlag != null">#{memberFlag},</if>
            <if test="perfectFlag != null">#{perfectFlag},</if>
            <if test="perfectTime != null">#{perfectTime},</if>
            <if test="probationTime != null">#{probationTime},</if>
            <if test="memberTime != null">#{memberTime},</if>
            <if test="memberExpiredTime != null">#{memberExpiredTime},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="userStatus != null">#{userStatus},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
         </trim>
    </insert>

    <update id="updateNjtUserRegister" parameterType="NjtUserRegister">
        update njt_user_register
        <trim prefix="SET" suffixOverrides=",">
            <if test="companyName != null">company_name = #{companyName},</if>
            <if test="mobilePhone != null">mobile_phone = #{mobilePhone},</if>
            <if test="password != null">password = #{password},</if>
            <if test="registerTime != null">register_time = #{registerTime},</if>
            <if test="memberFlag != null">member_flag = #{memberFlag},</if>
            <if test="perfectFlag != null">perfect_flag = #{perfectFlag},</if>
            <if test="perfectTime != null">perfect_time = #{perfectTime},</if>
            <if test="probationTime != null">probation_time = #{probationTime},</if>
            <if test="memberTime != null">member_time = #{memberTime},</if>
            <if test="memberExpiredTime != null">member_expired_time = #{memberExpiredTime},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="userStatus != null">user_status = #{userStatus},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteNjtUserRegisterById" parameterType="Long">
        delete from njt_user_register where id = #{id}
    </delete>

    <delete id="deleteNjtUserRegisterByIds" parameterType="String">
        delete from njt_user_register where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>
